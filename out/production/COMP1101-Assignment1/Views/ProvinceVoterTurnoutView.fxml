<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="751.0" prefWidth="1549.0" stylesheets="@../styleSheet.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ProvinceVoterTurnoutController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="751.0" prefWidth="1549.0" spacing="30.0" stylesheets="@../styleSheet.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="2019 Voter Turnout By Province" />
            <BarChart fx:id="electionBarChart" prefHeight="644.0" prefWidth="1480.0" stylesheets="@../styleSheet.css">
              <xAxis>
                <CategoryAxis fx:id="provinceXAxis" prefHeight="120.0" prefWidth="1428.0" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="avgYAxis" side="LEFT" />
              </yAxis>
            </BarChart>
            <Button mnemonicParsing="false" onAction="#viewBarChart" text="View Data" />
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
